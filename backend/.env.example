# Path of Exile session cookies (required)
POESESSID=your_session_id_here
CF_CLEARANCE=your_cloudflare_clearance_here

# Optional rate limiter tuning
# Ratio (0-1) above which soft throttle kicks in (default 0.8)
POE_SOFT_RATIO=0.8
# Fraction of remaining window to sleep for soft throttle (default 0.05)
POE_SOFT_SLEEP_FACTOR=0.05
